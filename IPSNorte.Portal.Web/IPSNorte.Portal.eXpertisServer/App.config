<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

  <system.runtime.remoting>
    <application>
      <service>
        <wellknown
          type="IPSNorte.Portal.eXpertisObjects.UserService, IPSNorte.Portal.eXpertisObjects"
          objectUri="UserService"
          mode="SingleCall" />
      </service>
      <channels>
        <channel ref="tcp" port="33000"/>
      </channels>
    </application>
  </system.runtime.remoting>
</configuration>